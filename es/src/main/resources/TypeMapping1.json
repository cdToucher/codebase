          "cvr": {
            "type": "double"
          },
          "effect_pv": {
            "type": "long"
          },
          "exposure_count": {
            "type": "long"
          },
          "launch_count": {
            "type": "long"
          },
          "participate_pv": {
            "type": "long"
          },
          "participate_uv": {
            "type": "long"
          },
          "promote_visit_pv": {
            "type": "long"
          },
          "promote_visit_uv": {
            "type": "long"
          },
          "sdk_uv": {
            "type": "long"
          },
          "slot_id": {
            "type": "long"
          },
          "visit_rate": {
            "type": "double"
          }
        }
      }
    }
  }
